[MESSAGES CONTROL]
disable=
  too-many-arguments,
  too-many-locals,
  too-many-return-statements,
  too-many-branches,
  too-many-instance-attributes,
  too-many-statements,
  duplicate-code,
  logging-not-lazy,
  consider-using-f-string,
  fixme,
  import-error,
  unspecified-encoding,
  too-few-public-methods,
  import-outside-toplevel,
  no-name-in-module

[FORMAT]
max-line-length=120
indent-string='    '
init-import=yes

[REFACTORING]
max-nested-blocks=6

[MASTER]
ignore=setup.py
ignore-paths=^tracex/extraction/migrations/.*$,

